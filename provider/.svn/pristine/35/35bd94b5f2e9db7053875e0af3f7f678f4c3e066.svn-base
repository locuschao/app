.pagination {
    font-size: 12px;
    margin:3px auto;
    text-align: center;
    background:  repeat-x scroll center -26px transparent;
    height: 26px;
    line-height: 26px;
}

.pagination a {
    text-decoration: none;
    border: solid 1px #AAE;
    color: #15B;
}

.pagination a, .pagination span {
    padding: 0.3em 0.5em;
    margin-right: 5px;
    margin-bottom: 5px;
}

.pagination .current {
    background: #26B;
