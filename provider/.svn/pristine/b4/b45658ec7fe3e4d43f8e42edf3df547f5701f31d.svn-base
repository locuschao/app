.goodsTab2 { height:28px; background: url(/images/pack/bg_goodsTab01.gif) bottom repeat-x; padding-top:12px; font-size:18px;}
.goodsTab2 ul {}
.goodsTab2 ul li { float:left; background:#fef4ea;  line-height:28px; border-left:1px solid #96946d; border-right:1px solid #96946d; padding:0px 15px; margin-right:5px; color:#000;}
.goodsTab2 ul li.chooseTag { background:#000; float:left; padding:0px 15px; z-index:5; color:#999b9a;  }
.goodsTab2 li.chooseTag a { color:#999b9a;}
