}
.fpx-label-frame-dhlpacketplus .fpx-label-tracking-no {
	font-style: italic;
}
.fpx-label-dhlpacketplusforau .fpx-label-barcode {
	width: 100%;
}
.fpx-label-dhlpacketplusforau .fpx-label-barcode img {
	border: none;
	padding-top: 0;
}
.fpx-label-dhlpacketplusforau .fpx-label-dhlpacketplus-recommande {
/* 	font-weight: bold; */
}
/* -------------- 从14年12月22号开始,后面做的标签的样式都放在这下面,标签上的元素不在写到element中再回到标签中,而是直接写到模板中 ---------*/
/* 美E通模板样式  */
.epac-indicator {
	width: 47px;
	height: 47px;
	line-height: 47px;
	font-size: 42px;
	font-weight: bold;
	text-align: center;
	border: 1px solid #000;
}
.epac-logo {
	padding-left: 30px;
	line-height: 12px;
}
.epac-logo img {
	width: 0.9in;
}
.epac-sg-logo {
	border-bottom: 1px solid #000;
}
.epac-sg-logo img {
	height: 0.3in;
}
.epac-usps-logo img {
	height: 0.2in;
}
.epac-returnSign {
	padding-left: 10px;
}
.label-frame-epac .epac-returnSign img {
	/* width: 20px;
	height: 20px; */
}
.epac-air {
	font-size: 8px;
	border: 1px solid #000;
	line-height: 9px;
	padding: 1px 8px;
}
.epac-pickup-zone {
	font-size: 14px;
	font-weight: bold;
	text-align: center;
}
.epac-return-address,
.epac-ra-state {
	font-size: 6px;
	line-height: 7px;
	width: 105px;
}
.epac-ra-state {
	padding-top: 8px;
	font-weight: bold;
}
.epac-zip {
	width: 169px;
	text-align: center;
	font-size: 8px;
	line-height: 8px;
}
.epac-zip div {
	padding-bottom: 1px;
}
.epac-zip img {
	width: 100px;
	height: 36px;
	margin: 15px 0;
}
.epac-barcode {
	font-size: 12px;
	line-height: 13px;
	font-weight: bold;
	text-align: center;
	width: 275px;
}
.epac-barcode img {
	width: 220px;
	height: 60px;
	margin: 15px 0;
}
.epac-to {
	width: 30px;
	text-align: center;
	padding-top: 8px;
}
.epac-from {
	font-size: 8px;
	line-height: 8px;
	padding-bottom: 1px;
}
.epac-consignee-addr {
	width: 240px;
	font-size: 8px;
	line-height: 9px;
}
.epac-barcode-state {
	padding-top: 1px;
}
.epac-trackingno {
	padding-bottom: 1px;
}
/* 美E通报关单模板样式  */
.fpx-declaration-frame.fpx-declaration-epac .fpx-declaration-main {
	font-size: 8px;
	border: none;
	padding: 8px 1px;
}
.declaration-epac-logo {
	font-size: 6px;
	line-height: 7px;
}
.declaration-epac-logo img {
	width: 80px;
	height: 36px;
}
.declaration-epac-barcode {
	font-weight: bold;
	font-size: 10px;
	text-align: center;
}
.declaration-epac-barcode img {
	width: 120px;
	height: 40px;
}
.declaration-epac-logo-py {
	text-align: center;
	font-weight: bold;
	font-size: 16px;
}
.declaration-epac-logo-py img {
	width: 73px;
	height: 36px;
}
.declaration-epac-return-addr {
	width: 140px;
	font-size: 8px;
	line-height: 9px;
}
.declaration-epac-contactno {
	padding-top: 10px;
}
.declaration-epac-senderref {
	border-top: 1px solid #000;
}
.declaration-epac-consigneeAddr {
	border-top: 1px solid #000;
	border-left: 1px solid #000;
	width: 140px;
	height: 83px;
	position: relative;
}
.declaration-epac-consigneeAddr  div {
	position: absolute;
	bottom: 3px;
}
.declaration-epac-no {
	width: 15px;
	text-align: center;
}
.declaration-epac-name {
	width: 120px;
}
.declaration-epac-qty {
	width: 17px;
	text-align: center;
}
.declaration-epac-kg {
	width: 20px;
}
.declaration-epac-val {
	width: 45px;
}
.declaration-epac-totalWeight {
	width: 138px;
}
.declaration-epac-totalWeight-value {
	width: 20px;
}
.declaration-epac-totalPrice-value {
	width: 45px;
}
.declaration-epac-value .declaration-epac-no,
.declaration-epac-value .declaration-epac-name,
.declaration-epac-value .declaration-epac-qty,
.declaration-epac-value .declaration-epac-kg,
.declaration-epac-value .declaration-epac-val,
.declaration-epac-value .declaration-epac-origin {
/* 	height: 15px; */
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.declaration-epac-sig {
	font-weight: bold;
	padding-top: 3px;
}
.declaration-epac-sig span {
	text-align: right;
	font-size: 12px;
	float: right;
}


@media screen {
	.fpx-label-label .fpx-label-container {
		margin-bottom: 5px;
	}
}
/* 打印时不打印border */
@media print {
	.fpx-label-frame {
		border: none;
	}
}