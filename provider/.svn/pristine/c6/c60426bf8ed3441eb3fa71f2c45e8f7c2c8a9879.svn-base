/* 标签框架的样式，即每一个标签最外层框架 */
.fpx-label-frame {
	width: 75mm;
	height: 81mm;
 	/* width: 304px;
	height: 340px; */
	border: 1px dotted gray;
	margin: 0 auto;
	position: relative;
}
.fpx-label-frame-main,
.fpx-label-frame-bottom-main {
/* 	padding: 3.5mm; */
	padding: 5px 4px;
}
.fpx-label-frame-bottom {
	position: absolute;
	width: 100%;
	bottom: 0;
	left: 0;
}
.fpx-label-row {
	width: 100%;
	overflow: hidden;
}
.fpx-label-row-relative {
	position: relative;
}
.fpx-label-blankRow {
	height: 20px;
}
.fpx-label-col-right-bottom {
	position: absolute;
	right: 0;
	bottom: 0;
}
.fpx-label-col-border, .fpx-label-row-border,
.fpx-label-col-row-border {
	border: 1px solid black;
	margin-bottom: 2px;
}
.fpx-label-col-row {
	width: 100%;
	overflow: hidden;
}
.fpx-label-row-top-bottom-border {
	border-top: 1px solid black;
	border-bottom: 1px solid black;
}
.fpx-label-row-top-bottom-border-2 {
	border-top: 2px solid black;
	border-bottom: 2px solid black;
}
.fpx-label-row-bottom-border {
	border-bottom: 1px solid black;
}
.fpx-label-col-right-border {
	border-right: 1px solid black;
}
.fpx-label-col-left-border {
	border-left: 1px solid black;
}
.fpx-label-col-left-right-border {
	border-right: 1px solid black;
	border-left: 1px solid black;
}
.fpx-label-col {
	width: auto;
	height: auto;
	float: left;
}
.fpx-label-col-center {
	width: 100%;
	text-align: center;
}
.fpx-label-col-right {
	float: right;
}
/*label 分页*/
.fpx-label-label .fpx-label-clear,
.fpx-label-label .fpx-label-page-break {
	display: none;
}
.fpx-label-clear-c {
	clear: both;
}
.fpx-label-page-break-c {
	page-break-after: always;
}
.fpx-label-label .fpx-label-clear-c,
.fpx-label-label .fpx-label-page-break-c {
	display: block;
}
.fpx-label-clear-c,
.fpx-label-page-break-c {
	display: none;
}
.fpx-label-label .fpx-label-clear-a4,
.fpx-label-label .fpx-label-page-break-a4,
.fpx-label-label .fpx-label-page-top-maring {
	display: none !important;
}
.fpx-label-label-container + .fpx-label-clear,
.fpx-label-label-container + .fpx-label-clear + .fpx-label-page-break,
.fpx-declaration-container-show + .fpx-label-clear,
.fpx-declaration-container-show + .fpx-label-clear + .fpx-label-page-break,
.fpx-peihuolist-container-show + .fpx-label-clear,
.fpx-peihuolist-container-show + .fpx-label-clear + .fpx-label-page-break {
	display: block;
}
.fpx-declaration-container-blank + .fpx-label-clear,
.fpx-declaration-container-blank + .fpx-label-clear + .fpx-label-page-break {
	display: none;
}
/* a4纸张样式 */
.fpx-label-a4 {
/* 	width: 814px; */
	width: 170mm;
	margin: 0 auto;
}
.fpx-label-a4 .fpx-label-content {
/* 	padding: 20px 10px; */
	padding: 0 5px;
	overflow: hidden;
}
.fpx-label-a4 .fpx-label-container {
/* 	width: 358px; */
	border: 1px dotted gray;
	margin: 1px auto 13px;
}
.fpx-label-a4 .fpx-order-container {
	/* 	width: 397px; */
		margin: 0 auto;
		padding: 0 15px;
}
.fpx-label-a4 .fpx-order-container {
	float: left;
}
.fpx-label-a4 .fpx-order-container-two-content {
	width: 633px;
	padding: 0;
}
.fpx-label-a4 .fpx-order-container-two-content .fpx-label-container {
	float: left;
	margin: 1px 15px 13px;	
}
.fpx-label-a4 .fpx-order-container-two-content .fpx-declaration-blank {
	display: block;
}
.fpx-label-label .fpx-declaration-blank,
.fpx-label-a4 .fpx-declaration-blank {
	display: none;
}
.fpx-label-a4 .fpx-label-frame {
	border: none;
}
.fpx-label-a4 .fpx-label-page-break,
.fpx-label-a4 .fpx-label-clear {
	display: none !important;
}
.fpx-label-clear-a4 {
	clear: both;
}
/* .fpx-label-a4 .fpx-label-page-break-a4,
.fpx-label-a4 .fpx-label-clear-a4 {
	display: block;
} */
.fpx-label-page-break,
.fpx-label-page-break-a4 {
	/* width: 100%;
	height: 42px;
	float: left;
	border-bottom: 1px dotted gray;
	margin-bottom: 20px; */
	page-break-after: always;
}
.fpx-label-clear-a4,
.fpx-label-page-top-maring,
.fpx-label-page-break-a4,
.fpx-label-page-bottom-maring  {
	display: none;
}
.fpx-order-container-two-content-page + .fpx-label-page-top-maring,
.fpx-order-container-two-content-page + .fpx-label-page-top-maring + .fpx-label-clear-a4,
.fpx-order-container-two-content-page + .fpx-label-page-top-maring + .fpx-label-clear-a4 + .fpx-label-page-break-a4,
.fpx-order-container-two-content-page + .fpx-label-page-top-maring + .fpx-label-clear-a4 + .fpx-label-page-break-a4 + .fpx-label-page-bottom-maring,
.fpx-order-container-one-content-page + .fpx-label-page-top-maring,
.fpx-order-container-one-content-page + .fpx-label-page-top-maring + .fpx-label-clear-a4,
.fpx-order-container-one-content-page + .fpx-label-page-top-maring + .fpx-label-clear-a4 + .fpx-label-page-break-a4,
.fpx-order-container-one-content-page + .fpx-label-page-top-maring + .fpx-label-clear-a4 + .fpx-label-page-break-a4 + .fpx-label-page-bottom-maring,
.fpx-order-container-three-content-page + .fpx-label-page-top-maring,
.fpx-order-container-three-content-page + .fpx-label-page-top-maring + .fpx-label-clear-a4,
.fpx-order-container-three-content-page + .fpx-label-page-top-maring + .fpx-label-clear-a4 + .fpx-label-page-break-a4,
.fpx-order-container-three-content-page + .fpx-label-page-top-maring + .fpx-label-clear-a4 + .fpx-label-page-break-a4 + .fpx-label-page-bottom-maring {
	display: block;
}
.fpx-label-page-top-maring,
.fpx-label-page-bottom-maring {
	width: 100%;
/* 	height: 20px; */
}
.fpx-label-page-last {
	display: none !important;
}
/* hnxb 个别样式 */
.fpx-label-frame-hnxb .fpx-label-insurance-sign, 
.fpx-label-frame-hnxb .fpx-label-return-sign, 
.fpx-label-frame-hnxb .fpx-label-gjz-sign {
	width: 15px;
	height: 13px;
}
.fpx-label-frame-hnxb .fpx-battery-img {
	width: 10px;
	height: 10px;
	margin: 1px;
}
/* sggh 个别样式 */
.fpx-label-frame-sggh .fpx-label-tracking-no {
	padding-left: 70px;
}
.fpx-label-frame-sggh .fpx-label-barcode {
	width: auto;
}
.fpx-label-frame-sggh .fpx-label-pickup-end,
.fpx-label-frame-sgpy .fpx-label-pickup-end  {
	display: block;
}
/* fjxb 个别样式  */
.fpx-label-frame-fjxb .fpx-label-orderNo span {
	display: none;
}
/* 中国EMS 个别样式*/
.fpx-label-frame-chems .fpx-label-orderNo span {
	display: none;
}
/* hkxb 个别样式*/
.fpx-label-frame-hkxb .fpx-label-gh-sign {
	float: left;
}
.fpx-label-frame-hkxb .fpx-label-tracking-no {
	margin-left: 40px;
	display: block;
}
.fpx-label-frame-hkxb .fpx-label-tracking-no {
	font-size: 12px;
}
.fpx-label-frame-hkxb .fpx-label-consignee-info {
	width: 160px;
}
.fpx-label-frame-hkxb .fpx-label-pickup-zone {
	overflow: hidden;
	width: 72px;
}
.fpx-label-frame-hkxb .fpx-label-pickup-zone .fpx-label-pickup-zone-main {
	float: right;
}
.fpx-label-frame-hkxb .fpx-label-pickup-end {
	display: block;
	float: right;
	width: 56px;
}
.fpx-label-frame-hkxb .fpx-label-charge-zone {
	height: auto;
	line-height: normal;
	margin-left: 10px;
}
.fpx-label-frame-hkxb .fpx-label-barcode {
	width: auto;
}

/* 中国邮政广州 个别样式  */
.fpx-label-frame-chgz .fpx-label-barcode {
	width: 210px;
}
.fpx-label-frame-chgz .fpx-label-ch-air-sign {
	padding: 0 10px;
}
.fpx-label-chgz-ghSign-width {
	width: 22px;
}
/* 福州小包 */
.fpx-label-col-fzxb .fpx-label-charge-zone {
	height: 20px;
	line-height: 20px;
}
/* finland */
.fpx-label-frame-finland .fpx-label-insurance-sign,
.fpx-label-frame-finland .fpx-label-return-sign,
.fpx-label-frame-finland .fpx-label-gjz-sign {
	width: 18px;
	height: 15px;
	margin-bottom: 25px;
}
.fpx-label-frame-finland .fpx-label-pickup-zone .fpx-label-pickup-zone-main {
	width: 30px;
	margin-left: 3px;
}
.fpx-label-frame-finland .fpx-label-pickup-zone-code {
	font-size: 14px;
}
/* DHL小包挂号特别样式  */
.fpx-label-frame-dhlpacketplus .fpx-label-barcode {
	width: 210px;
}
.fpx-label-frame-dhlpacketpy .fpx-label-barcode {
	width: 100%;
}
.fpx-label-frame-dhlpacketplus .fpx-label-gh-sign {
	font-size: 50px;
	padding-right: 5mm;
	padding-left: 0;
}
.fpx-label-frame-dhlpacketplus .fpx-label-barcode img {
	padding-top: 1.5mm;
	border-top: 2px solid #000;
	height: 7mm;
}
.fpx-label-frame-dhlpacketpy .fpx-label-barcode img {
