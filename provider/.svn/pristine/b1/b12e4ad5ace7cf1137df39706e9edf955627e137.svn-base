function setMainWarehouseDialog() {    var html = '<table class="table-module" border="0" cellpadding="0" cellspacing="0" style="float:left;width:100%" id="user_warehouse_list">';    html += "<tr><td width='100%' class='ec-center'>操作仓库</td></tr>";    $.ajax({        type: "POST",        dataType: "json",        async: false,        url: "/default/system/set-warehouse",        success: function (json) {            if (json.state == '1') {                var current_operation_warehouse = $.cookie('current_operation_warehouse');                var tag_tr_class = '';                var ck = '';                $.each(json.data, function (k, v) {                    tag_tr_class = ((k + 1) % 2 == 1) ? "table-module-b2" : "table-module-b1";                    ck = current_operation_warehouse == v.warehouse_id ? "checked" : '';                    html += "<tr class='" + tag_tr_class + "'><td><label><input type='radio' " + ck + "  value='" + v.warehouse_id + "'  class='current_operation_warehouse' name='current_operation_warehouse' >[" + v.warehouse_code + ']' + "&nbsp;&nbsp;" + v.warehouse_desc + "</label></td></tr>";                });                html += '</table>';            } else {                html = '<tr><td>您没有权限操作</td></tr>';            }        }    });    html += '</table>';    $('<div title="设置仓库" id="dialog-auto-alert-tip">' + html + '</div>').dialog({        autoOpen: true,        width: 350,        _minHeight: 80,        height: 300,        modal: true,        show: "slide",        buttons: [            {                text: '关闭(Close)',                click: function () {                    $(this).dialog("close");                }            }        ],        close: function () {            closeCallback();            closeCallbackFun();            $(this).detach();        }    });}function initSetWarehouse() {    if ($.cookie('current_operation_warehouse') == '' || $.cookie('current_operation_warehouse') == null) {        _location_reload = false;        setMainWarehouseDialog();    } else {        _location_reload = true;    }}function closeCallback() {    initSetWarehouse();}function closeCallbackFun() {    //是否刷新页面    if ("undefined" != typeof _location_reload && _location_reload) {        window.location.reload();    }}$(function () {    setTimeout(initSetWarehouse, 500);    //选择仓库    $(".current_operation_warehouse").live("click", function () {        $.cookie('current_operation_warehouse', $(this).val(), {expires: 365, path: '/'})    });});