@charset "utf-8";
body {
	font-size: 10px;
	font-family: Arial, Helvetica, sans-serif;
}
p{
	margin: 0px;
	padding: 0px;
}
ul{
	margin: 0px;
	padding: 0px;
    list-style-type: none;
}
li{
	margin: 0px;
	padding: 0px;
}
.table {
	BORDER-SPACING: 0; BORDER-COLLAPSE: collapse
}
th{
	margin: 0px;
	padding: 0px;
}
td{
	margin: 0px;
	padding: 0px;
}
 .inputcheckbox{vertical-align:middle;}
  label{
	vertical-align:middle;
	font-size: 11px;
}
#bgd{
	height: 75mm;
	width: 75mm;
	font-size: 10px;
	border-left: 0px;
	border-right: 0px;
	border-bottom: 0px;
	border-top: 0px;
	text-align:left;
}
.part1{
	width: 74.4mm;
	border: 1px solid #000;
	height: 9mm;
}
.part1 li{}
.part1_1{
	float: left;
	width: 30mm;
	font-size: 14px;
	line-height: 16px;
	font-weight: normal;
	padding-top: 0.3mm;
	padding-left: 3mm;
}
.part1_2{
	float: left;
	width: 22mm;
	height: auto;
	padding-top: 2mm;
	font-size: 10px;
}
.part1_3{
	float: left;
	width: 15mm;
	height: auto;
	font-size: 16px;
	line-height: 30px;
	color: #000;
}
.part2{
	width: 74.4mm;
	height: 8mm;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-right-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	border-top-color: #000;
	border-right-color: #000;
	border-bottom-color: #000;
	border-left-color: #000;
}
.part2 span{}
.part2_1{
	float: left;
	width: 40mm;
	padding-left: 3mm;
	font-size: 10px;
	padding-top: 0.3mm;
}
.part2_2{
	float: left;
	width: 30mm;
	font-size: 14px;
	padding-top: 1mm;
	line-height: 17px;
	
}
.part2_3{
	font-size: 8px;
	line-height: 9px;
}
.part3{
	width: 74.4mm;
	height: 10mm;
	border-right-width: 1px;
	border-left-width: 1px;
	border-right-style: solid;
	border-left-style: solid;
	border-right-color: #000;
	border-left-color: #000;
}
.part3 li{
	margin: 0px;
	padding: 0px;
}
.part3_1{
	float: left;
	width: 30mm;
	padding-left: 3mm;
	font-size: 10px;
	line-height: 12px;
}
.part3_2{
	float: left;
	width: 43mm;
	font-size: 10px;
	line-height: 14px;
}
.part3_3{
	font-size: 8px;
	font-style: italic;
}
.part4{
	float: left;
	height: auto;
	width: 75mm;
}
.table {
	WIDTH: 100%;
	BACKGROUND: #f9f9f9;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-right-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	border-right-color: #000;
	border-bottom-color: #000;
	border-left-color: #000;
	font-size: 8px;
}
.table TH {
	BORDER-BOTTOM: #000 1px solid;
	BORDER-LEFT: #000 1px solid;
	PADDING-BOTTOM: 5px;
	PADDING-LEFT: 5px;
	PADDING-RIGHT: 5px;
	BORDER-TOP: #000 1px solid;
	BORDER-RIGHT: #000 1px solid;
	PADDING-TOP: 5px;
	text-align: right;
}
.table TD {
	BORDER-BOTTOM: #000 1px solid;
	BORDER-LEFT: #000 1px solid;
	PADDING-BOTTOM: 2px;
	PADDING-LEFT: 5px;
	PADDING-RIGHT: 5px;
	BORDER-TOP: #000 1px solid;
	BORDER-RIGHT: #000 1px solid;
	PADDING-TOP: 2px;
	text-align: left;
	background-color: #FFF;
}
.table TH {
	TEXT-ALIGN: right;
	WIDTH: 190px;
	FONT-WEIGHT: bold
}
.table .th {
	font-size: 8px;
	line-height: 10px;
}
.part5{
	float: left;
	height: auto;
	width: 75mm;
}
